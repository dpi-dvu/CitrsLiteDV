<MudPaper>
    <MudNavMenu Color="Color.Primary" Margin="Margin.Normal" Bordered="true">
        <MudContainer Class="my-2">
            <MudText Typo="Typo.h3" Class="px-2" Align="Align.Center" Style="font-weight:bold">CITRS</MudText>
            <MudText Typo="Typo.body2" Class="px-4 mud-text-secondary mb-2" Align="Align.Center">
                Citrus Indexing, Tracking, and Registration System
            </MudText>
        </MudContainer>
        <MudDivider />
        <MudNavLink Href="" Match="NavLinkMatch.All" Icon="@Icons.Material.Filled.Home">Home</MudNavLink>
        <MudNavGroup Title="Participants" Icon="@Icons.Material.Filled.People" Expanded="false">
            <MudNavLink Href="/participant/add" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Add">Add Participant</MudNavLink>
            <MudNavLink Href="/participant/search" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Search">Search Participant</MudNavLink>
        </MudNavGroup>
        <MudNavGroup Title="Variety/Clones" Icon="@Icons.Material.Filled.Lens" IconColor="Color.Warning" Expanded="false">
            <MudNavLink Href="/varietyClone/add" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Add">Add Variety/Clone</MudNavLink>
            <MudNavLink Href="/varietyClone/search" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Search">Search Variety/Clone</MudNavLink>
        </MudNavGroup>
        <MudNavGroup Title="Trees" Icon="@Icons.Material.Filled.Nature" IconColor="Color.Success" Expanded="false">
            <MudNavLink Href="/tree/add" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Add">Add Tree</MudNavLink>
            <MudNavLink Href="/tree/search" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Search">Search For Tree</MudNavLink>
        </MudNavGroup>
        <MudNavGroup Title="Budwood Cutting Reports" Icon="@Icons.Material.Filled.EditNote" Expanded="false">
            <MudNavLink Href="/bcr/add" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Add">New BCR</MudNavLink>
            <MudNavLink Href="/bcr/search" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Search">Search BCR</MudNavLink>
        </MudNavGroup>
    </MudNavMenu>
</MudPaper>